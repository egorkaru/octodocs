{
  "name": "octodocs",
  "version": "0.0.4",
  "description": "Агрегатор OpenAPI документации для микросервисов",
  "main": "dist/server/server.js",
  "scripts": {
    "build-clean": "rm -rf dist/",
    "dev-clean": "find src/ -type f -name \"*.js\" -delete",
    "watch-server": "tsc --project tsconfig.server.json --watch",
    "build-server": "tsc --project tsconfig.server.json --outDir dist/server",
    "dev": "concurrently \"yarn watch-server\" \"yarn dev-app\"",
    "dev-app": "node src/server.js",
    "build-app": "NODE_ENV=production node dist/server/server/bundle.js --build --in ../../../src/App.tsx --out ../../client",
    "build": "yarn build-clean; yarn build-server; yarn build-app;",
    "start": "NODE_ENV=production node dist/server/server.js",
    "prepublishOnly": "yarn build",
    "build-docker-image": "docker build -t octodocs .",
    "build-docker": "yarn build; yarn build-docker-image;",
    "start-docker": "docker run --rm -it -p 8080:8080 octodocs"
  },
  "bin": "./bin/octodocs.js",
  "author": "Egor Rudinsky <egor@egor.win>",
  "repository": "github:egorkaru/octodocs",
  "license": "Apache-2.0",
  "private": false,
  "bugs": {
    "url": "https://github.com/egorkaru/octodocs/issues"
  },
  "homepage": "https://github.com/egorkaru/octodocs#readme",
  "dependencies": {
    "@kubernetes/client-node": "^0.8.2",
    "axios": "^0.18.0",
    "dotenv": "^7.0.0",
    "http-proxy": "^1.17.0",
    "micro": "^9.3.3",
    "micro-route": "^2.5.0",
    "serve-handler": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/http-proxy": "^1.17.0",
    "@types/micro": "^7.3.3",
    "@types/parcel-bundler": "^1.12.0",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/styled-jsx": "^2.2.8",
    "concurrently": "^4.1.0",
    "micro-dev": "^3.0.0",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "styled-jsx": "^3.2.1",
    "swagger-ui": "^3.22.1",
    "typescript": "^3.3.3"
  }
}
