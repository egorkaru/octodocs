{
  "name": "octodocs",
  "version": "0.0.1",
  "description": "Агрегатор OpenAPI документации для микросервисов",
  "main": "src/server.js",
  "scripts": {
    "watch-server": "tsc --project tsconfig.server.json --watch",
    "build-server": "tsc --project tsconfig.server.json",
    "dev": "concurrently \"yarn watch-server\" \"yarn dev-next\"",
    "dev-next": "micro-dev --watch src/server.js",
    "build-next": "next build src",
    "build": "yarn build-server; yarn build-next",
    "start": "NODE_ENV=production micro src/server.js"
  },
  "repository": "git@github.com:egorkaru/octodocs.git",
  "author": "Egor Rudinskiy <egor@egor.win>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@kubernetes/client-node": "^0.8.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.18.0",
    "micro": "^9.3.3",
    "micro-route": "^2.5.0",
    "next": "^8.0.1",
    "next-compose-plugins": "^2.1.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "swagger-ui": "^3.22.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/micro": "^7.3.3",
    "@types/next": "^8.0.1",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/styled-jsx": "^2.2.8",
    "concurrently": "^4.1.0",
    "micro-dev": "^3.0.0",
    "typescript": "^3.3.3"
  }
}
